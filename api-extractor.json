{
    "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
    "projectFolder": "./",
    "mainEntryPointFilePath": "<projectFolder>/dist/main.d.ts",
    "apiReport": {
        "enabled": false
    },
    "dtsRollup": {
        "enabled": false
    },
    "docModel": {
        "enabled": true
    },
    "messages": {
        "tsdocMessageReporting": {
            "tsdoc-undefined-tag": {
                "logLevel": "none"
            }
        }
    }
}
